{
  "name": "@ribajs/nest-lunr",
  "version": "2.0.0-alpha.0",
  "description": "Nest.js module to use Lunr.js in your in your Nest.js application.",
  "author": "Art+Code Studio (Pascal Garber) <pascal@artandcode.studio>",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "clear": "rimraf dist",
    "build": "npm run lint && tsc -p tsconfig.json",
    "watch": "npm run build -- --watch",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --ignore-pattern '*.spec.ts' --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@types/express": "^4.17.11",
    "lunr": "^2.3.9",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7"
  },
  "peerDependencies": {
    "pug": "^3.0.0",
    "tinyliquid": "^0.2.34",
    "twig": "^1.15.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.15",
    "@types/cookie-parser": "^1.4.2",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
