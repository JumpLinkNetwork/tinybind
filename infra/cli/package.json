    "@ribajs/schematics": "^1.9.0-alpha.2",
    "@types/os-name": "^3.1.0",
    "@yarnpkg/pnpify": "^2.0.0-rc.20",
    "chalk": "^3.0.0",
    "cli-table3": "^0.6.0",
    "commander": "^5.0.0",
    "dashify": "^2.0.0",
    "debug": "^4.1.1",
