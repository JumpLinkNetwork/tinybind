    "keypath",
    "langcode",
    "linklist",
    "myshopify",
    "nestjs",
    "nodir",
    "octobercms",
    "pathnames",
    "photoswipe",
    "pjax",
    "pnpify",
    "prebuild",
    "preconnect",
    "prefetch",
    "regist",
    "ribajs",
    "sightglass",
