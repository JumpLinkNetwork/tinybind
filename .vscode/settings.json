{
  "editor.tabSize": 2,
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "eslint.nodePath": ".yarn/sdks",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "peacock.color": "#37a07e",
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "cSpell.words": [
    "Autoscroll",
    "Childs",
    "Entrypoints",
    "Garber",
    "Riba",
    "Scrollable",
    "Scrollend",
    "autobind",
    "autoscrolling",
    "builtins",
    "colorpicker",
    "corejs",
    "draggable",
    "dragscroll",
    "easdk",
    "fadeshow",
    "favicons",
    "fullhuman",
    "handleize",
    "iconset",
    "indexeddb",
    "inspectpack",
    "keypath",
    "langcode",
    "linklist",
    "myshopify",
    "nestjs",
    "nodir",
    "octobercms",
    "pathnames",
    "photoswipe",
    "pjax",
    "pnpify",
    "popperjs",
    "prebuild",
    "preconnect",
    "prefetch",
    "purgecss",
    "regist",
    "ribajs",
    "scrollended",
    "scrollspy",
    "sightglass",
    "sphinxxxx",
    "submodules",
    "subresource",
    "taggedimage",
    "tinybind",
    "tinyliquid",
    "toggleable",
    "typecheck",
    "webpackbar",
    "xlink",
    "yarnpkg"
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#49c19a",
    "activityBar.activeBorder": "#9541be",
    "activityBar.background": "#49c19a",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#9541be",
    "activityBarBadge.foreground": "#e7e7e7",
    "statusBar.background": "#37a07e",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#49c19a",
    "titleBar.activeBackground": "#37a07e",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#37a07e99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modifications",
  "typescript.format.enable": false,
  "eslint.format.enable": true,
  "eslint.packageManager": "yarn",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
}
