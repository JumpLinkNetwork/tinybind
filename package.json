    "@babel/preset-env": "^7.0.0-beta.56",
    "@babel/preset-typescript": "^7.0.0-beta.56",
    "@types/jquery": "^3.3.5",
    "babel-loader": "^8.0.0-beta.4",
    "harp": "^0.27.0",
    "mocha": "^5.2.0",
    "mocha-chrome": "^1.1.0",
    "should": "^13.2.3",
    "sinon": "^6.1.4",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.56",
    "@types/core-js": "^2.5.0",
