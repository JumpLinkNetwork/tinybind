  "contributors": [
    "Michael Richards",
    "Luiz Américo Pereira Câmara"
  ],
  "url": "https://github.com/blikblum/tinybind",
  "main": "./src/index.ts",
  "browser": "./dist/tinybind.min.js",
  "source": "./src/index.ts",
