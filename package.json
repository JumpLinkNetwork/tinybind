{
  "name": "tinybind",
  "description": "Declarative data binding + templating solution.",
  "version": "0.15.0",
  "author": "Pascal Garber <pascal@jumplink.eu>",
  "contributors": [
    "Michael Richards",
    "Luiz Américo Pereira Câmara"
  ],
  "url": "https://github.com/blikblum/tinybind",
  "main": "./src/index.ts",
  "browser": "./dist/tinybind.min.js",
  "source": "./src/index.ts",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/blikblum/tinybind/blob/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/blikblum/tinybind.git"
  },
  "scripts": {
    "test": "mocha-chrome spec/runner.html && mocha-chrome spec/runner.min.html",
    "type-check": "tslint -c tslint.json 'src/**/*.ts' && tsc --noEmit",
    "build": "npm run type-check && npm run webpack",
    "webpack": "webpack && webpack --config webpack.dev.config.js",
    "watch": "webpack --watch --env.dev",
    "doc": "harp server docs/_harp ./docs",
    "build-doc": "harp compile docs/_harp ./docs"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-typescript": "^7.0.0-beta.54",
    "@types/jquery": "^3.3.5",
    "babel-loader": "^8.0.0-beta.4",
    "harp": "^0.27.0",
    "mocha": "^5.2.0",
    "mocha-chrome": "^1.1.0",
    "should": "^13.2.1",
    "sinon": "^6.1.4",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.54",
    "@types/core-js": "^2.5.0",
    "@types/debug": "0.0.30",
    "debug": "^3.1.0",
    "jquery": "^3.3.1"
  }
}
