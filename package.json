{
  "name": "ribajs",
  "description": "Declarative data binding + template solution.",
  "version": "2.0.0-alpha.0",
  "private": true,
  "author": "Art+Code Studio (Pascal Garber) <pascal@artandcode.studio>",
  "contributors": [],
  "main": "src/index.ts",
  "url": "https://github.com/ribajs/riba",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ribajs/riba/blob/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ribajs/riba.git"
  },
  "keywords": [
    "Bootstrap",
    "Bootstrap4",
    "Shopify",
    "Art+Code Studio",
    "Riba",
    "Rivets",
    "tinybind",
    "SPA",
    "TypeScript",
    "Browser"
  ],
  "scripts": {
    "workspace-foreach": "yarn node workflow/workspace-foreach.js",
    "reinstall": "YARN_CHECKSUM_BEHAVIOR=update yarn install",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --config=jest.config.js --watch",
    "test:clear": "jest --config=jest.config.js --clearCache",
    "test:migrate": "ts-jest config:migrate jest.config.js",
    "lint": "yarn workspace-foreach lint",
    "build": "yarn workspace-foreach build",
    "clean": "yarn workspace-foreach clean",
    "bump:prerelease": "yarn workspaces foreach version prerelease",
    "bump:patch": "yarn workspaces foreach version patch",
    "bump:minor": "yarn workspaces foreach version minor",
    "bump:major": "yarn workspaces foreach version major",
    "publish:next": "yarn run build && yarn workspaces foreach npm publish --tolerate-republish --tag next --access public",
    "publish:latest": "yarn run build && yarn workspaces foreach npm publish --tolerate-republish --tag latest --access public"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.13",
    "@babel/plugin-syntax-export-default-from": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "@babel/runtime-corejs3": "^7.12.13",
    "@babel/types": "^7.12.13",
    "@nestjs/common": "^7.6.12",
    "@nestjs/core": "^7.6.12",
    "@nestjs/testing": "^7.6.12",
    "@ribajs/eslint-config": "workspace:infra/eslint-config",
    "@ribajs/tsconfig": "workspace:infra/tsconfig",
    "@ribajs/types": "workspace:infra/types",
    "@types/core-js": "^2.5.4",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/experimental-utils": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "@yarnpkg/pnpify": "^2.4.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-array-includes": "^2.0.3",
    "core-js": "^3.8.3",
    "dart-sass": "^1.25.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-config": "^26.6.3",
    "jest-extended": "^0.11.5",
    "pkg-dir": "^5.0.0",
    "prettier": "2.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.4",
    "webpack-glob-entry": "^2.1.1"
  },
  "workspaces": [
    "packages/*",
    "infra/*",
    "!infra/cli",
    "!infra/doc",
    "!infra/schematics",
    "backend/*",
    "demos/*",
    "!demos/vue",
    "examples/*",
    "!examples/electron",
    "!examples/electron-nest"
  ],
  "dependenciesMeta": {
    "prettier": {
      "unplugged": true
    }
  },
  "resolutions": {
    "@types/express-serve-static-core": "latest",
    "@types/node": "latest",
    "@ribajs/bs4": "portal:packages/bs4",
    "@ribajs/cache": "portal:packages/cache",
    "@ribajs/core": "portal:packages/core",
    "@ribajs/extras": "portal:packages/extras",
    "@ribajs/events": "portal:packages/events",
    "@ribajs/history": "portal:packages/history",
    "@ribajs/i18n": "portal:packages/i18n",
    "@ribajs/iconset": "portal:packages/iconset",
    "@ribajs/jquery": "portal:packages/jquery",
    "@ribajs/moment": "portal:packages/moment",
    "@ribajs/photoswipe": "portal:packages/photoswipe",
    "@ribajs/router": "portal:packages/router",
    "@ribajs/shopify": "portal:packages/shopify",
    "@ribajs/shopify-easdk": "portal:packages/shopify-easdk",
    "@ribajs/shopify-tda": "portal:packages/shopify-tda",
    "@ribajs/utils": "portal:packages/utils",
    "@ribajs/doc": "portal:infra/doc",
    "@ribajs/eslint-config": "portal:infra/eslint-config",
    "@ribajs/types": "portal:infra/types",
    "@ribajs/webpack-config": "portal:infra/webpack-config",
    "@ribajs/tsconfig": "portal:infra/tsconfig",
    "@ribajs/nest-theme": "portal:backend/nest-theme"
  },
  "dependencies": {
    "glob": "^7.1.6",
    "pug": "^3.0.0"
  }
}
