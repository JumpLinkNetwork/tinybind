  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/blikblum/tinybind/blob/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/blikblum/tinybind.git"
  },
  "scripts": {
    "test": "mocha-chrome spec/runner.html && mocha-chrome spec/runner.min.html",
    "type-check": "tslint -c tslint.json 'src/**/*.ts' && tsc --noEmit",
    "build": "npm run type-check && npm run webpack",
    "webpack": "webpack && webpack --config webpack.dev.config.js",
