{
  "name": "ribajs",
  "description": "Declarative data binding + templating solution.",
  "version": "1.9.0-beta.3",
  "private": true,
  "author": "Pascal Garber <pascal@artandcode.studio>",
  "contributors": [],
  "main": "src/index.ts",
  "url": "https://github.com/ribajs/riba",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ribajs/riba/blob/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ribajs/riba.git"
  },
  "keywords": [
    "Bootstrap",
    "Bootstrap4",
    "Shopify",
    "Art+Code Studio",
    "Riba",
    "Rivets",
    "tinybind",
    "SPA",
    "TypeScript",
    "Browser"
  ],
  "scripts": {
    "reinstall": "YARN_CHECKSUM_BEHAVIOR=update yarn install",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --config=jest.config.js --watch",
    "test:clear": "jest --config=jest.config.js --clearCache",
    "test:migrate": "ts-jest config:migrate jest.config.js",
    "lint": "yarn workspaces foreach run lint",
    "build": "yarn workspaces foreach run build",
    "clean": "yarn workspaces foreach run clean",
    "bump:prerelease": "yarn version prerelease && yarn workspaces foreach version prerelease",
    "bump:patch": "yarn version patch && yarn workspaces foreach version patch",
    "bump:minor": "yarn version minor && yarn workspaces foreach version minor",
    "bump:major": "yarn version major && yarn workspaces foreach version major",
    "publish": "yarn run build && yarn workspaces foreach npm publish --tolerate-republish --tag latest --access public"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-syntax-export-default-from": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@babel/runtime-corejs3": "^7.12.1",
    "@babel/types": "^7.12.1",
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/testing": "^7.5.1",
    "@ribajs/eslint-config": "workspace:infra/eslint-config",
    "@ribajs/tsconfig": "workspace:infra/tsconfig",
    "@ribajs/types": "workspace:infra/types",
    "@types/core-js": "^2.5.4",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/experimental-utils": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "@yarnpkg/pnpify": "^2.3.3",
    "babel-jest": "^26.6.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-array-includes": "^2.0.3",
    "core-js": "^3.6.5",
    "dart-sass": "^1.25.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.2",
    "jest-config": "^26.6.2",
    "jest-extended": "^0.11.5",
    "pkg-dir": "^5.0.0",
    "prettier": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5",
    "webpack-glob-entry": "^2.1.1"
  },
  "husky": {
    "hooks": {
      "disabled: pre-push": "yarn run test",
      "disabled: pre-commit": "yarn run lint"
    }
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "demos/*",
      "!demos/vue",
      "infra/eslint-config",
      "infra/types",
      "infra/webpack-config",
      "infra/webpack-serve",
      "infra/tsconfig"
    ]
  },
  "resolutions": {
    "@ribajs/bs4": "portal:packages/bs4",
    "@ribajs/cache": "portal:packages/cache",
    "@ribajs/core": "portal:packages/core",
    "@ribajs/extras": "portal:packages/extras",
    "@ribajs/history": "portal:packages/history",
    "@ribajs/i18n": "portal:packages/i18n",
    "@ribajs/iconset": "portal:packages/iconset",
    "@ribajs/jquery": "portal:packages/jquery",
    "@ribajs/moment": "portal:packages/moment",
    "@ribajs/pdf": "portal:packages/pdf",
    "@ribajs/photoswipe": "portal:packages/photoswipe",
    "@ribajs/router": "portal:packages/router",
    "@ribajs/shopify": "portal:packages/shopify",
    "@ribajs/shopify-easdk": "portal:packages/shopify-easdk",
    "@ribajs/shopify-tda": "portal:packages/shopify-tda",
    "@ribajs/utils": "portal:packages/utils",
    "@ribajs/doc": "portal:infra/doc",
    "@ribajs/eslint-config": "portal:infra/eslint-config",
    "@ribajs/types": "portal:infra/types",
    "@ribajs/webpack-config": "portal:infra/webpack-config",
    "@ribajs/tsconfig": "portal:infra/tsconfig"
  }
}
