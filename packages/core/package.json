{
  "name": "@ribajs/core",
  "description": "Core module of Riba.js",
  "version": "0.16.5",
  "author": "Pascal Garber <pascal@jumplink.eu>",
  "private": false,
  "contributors": [
    "Michael Richards",
    "Luiz Américo Pereira Câmara"
  ],
  "url": "https://github.com/ribajs/riba",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ribajs/riba/blob/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ribajs/riba.git"
  },
  "scripts": {
    "test": "mocha-chrome spec/runner.html && mocha-chrome spec/runner.min.html",
    "type-check": "tslint -c tslint.json 'src/**/*.ts' && tsc --noEmit",
    "build": "tsc",
    "build:browser": "npm run type-check && npm run webpack:browser",
    "webpack:browser": "webpack && webpack --config webpack.dev.config.js",
    "watch:browser": "webpack --watch --env.dev",
    "doc": "harp server docs/_harp ./docs",
    "build-doc": "harp compile docs/_harp ./docs"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jquery": "^3.3.22",
    "babel-loader": "^8.0.4",
    "babel-plugin-array-includes": "^2.0.3",
    "harp": "^0.27.0",
    "mocha": "^5.2.0",
    "mocha-chrome": "^1.1.0",
    "should": "^13.2.3",
    "sinon": "^6.3.5",
    "tslint": "^5.11.0",
    "typescript": "^3.1.5",
    "webpack": "^4.23.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.1.2",
    "@types/core-js": "^2.5.0",
    "@types/debug": "^0.0.30",
    "debug": "^3.2.6",
    "jquery": "^3.3.1"
  },
  "bugs": {
    "url": "https://github.com/ribajs/riba/issues"
  },
  "homepage": "https://github.com/ribajs/riba#readme",
  "directories": {
    "lib": "lib"
  }
}
